{
  "uid": "render-latency",
  "title": "Render Pipeline",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Render Job Duration",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(render_duration_seconds_bucket[5m])) by (le))"
        }
      ],
      "gridPos": {"h": 9, "w": 12, "x": 0, "y": 0}
    },
    {
      "type": "timeseries",
      "title": "GPU Utilization",
      "targets": [
        {
          "expr": "avg(nvidia_gpu_duty_cycle)"
        }
      ],
      "gridPos": {"h": 9, "w": 12, "x": 12, "y": 0}
    }
  ]
}
